  <h1 id='11-4'>攻撃開始！</h1>
  <h2>自分の海域</h2>
  <table id='myfield'>
    <tbody>
      <tr>
        <td></td>
        <% (1..10).each do |i|%>
          <td><%= i %></td>
        <% end %>
      </tr>
      <% (0..9).each do |i| %>
        <tr><td><%= i+1 %></td>
          <% (0..9).each do |j| %>
          <% case @myfield[i][j] %>
          <% when 0 then %>
            <td id='m<%=i%>-<%=j%>-<%=@myfield[i][j]%>' data-id='<%=i%>-<%=j%>-<%=@myfield[i][j]%>' style="background-color: #00ffff"></td>
          <% when 1 then %>
            <td id='m<%=i%>-<%=j%>-<%=@myfield[i][j]%>' data-id='<%=i%>-<%=j%>-<%=@myfield[i][j]%>'style="background-color: #00ff00"></td>
          <% when 2 then %>
            <td id='m<%=i%>-<%=j%>-<%=@myfield[i][j]%>' data-id='<%=i%>-<%=j%>-<%=@myfield[i][j]%>'style="background-color: #f5f5f5"></td>
          <% when 3 then %>
            <td id='m<%=i%>-<%=j%>-<%=@myfield[i][j]%>' data-id='<%=i%>-<%=j%>-<%=@myfield[i][j]%>'style="background-color: #ff0000"></td>
          <% end %>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
  <h2><%=@room.opponent%>の海域</h2>
  <table id='oppoField'>
    <tbody>
      <tr>
        <td></td>
        <% (1..10).each do |i|%>
          <td><%= i %></td>
        <% end %>
      </tr>
      <% (0..9).each do |i| %>
        <tr>
          <td><%= i+1 %></td>
          <% (0..9).each do |j| %>
          <% case @hisfield[i][j] %>
          <% when 0 then %>
            <td class='unclicked' id='h<%=i%>-<%=j%>-<%=@hisfield[i][j]%>' data-id='<%=i%>-<%=j%>-<%=@hisfield[i][j]%>' style="background-color: #00ffff"></td>
          <% when 1 then %>
            <td class='unclicked' id='h<%=i%>-<%=j%>-<%=@hisfield[i][j]%>' data-id='<%=i%>-<%=j%>-<%=@hisfield[i][j]%>' style="background-color: #00ffff"></td>
          <% when 2 then %>
            <td class='clicked' id='h<%=i%>-<%=j%>-<%=@hisfield[i][j]%>' data-id='<%=i%>-<%=j%>-<%=@hisfield[i][j]%>' style="background-color: #f5f5f5"></td>
          <% when 3 then %>
            <td class='clicked' id='h<%=i%>-<%=j%>-<%=@hisfield[i][j]%>' data-id='<%=i%>-<%=j%>-<%=@hisfield[i][j]%>' style="background-color: #ff0000"></td>
          <% end %>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
  <button class='reset' style='visibility:hidden'>reset</button>
  <%=render 'battlejs'%>